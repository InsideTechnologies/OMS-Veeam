                                        "Query": "search * | where ( EventLog == \"Veeam Backup\" ) and EventLevelName == \"Error\" | where TimeGenerated > ago(7d) | summarize AggregatedValue = count() by Computer",
                                        "HideGraph": false,
                                        "enableSparklines": false,
                                        "operation": "Summary",
                                        "ColumnsTitle": {
                                            "Name": "Computer",
                                            "Value": "Count"
