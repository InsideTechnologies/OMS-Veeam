                                        "Query": "search * | where ( EventLog == \"Veeam Backup\" ) and EventLevelName == \"Error\" | where TimeGenerated > ago(7d) | summarize AggregatedValue = count() by EventLevelName",
                                        "CenterLegend": {
                                            "Text": "Total",
                                            "Operation": "Sum",
                                            "ArcsToSelect": []
                                        },
                                        "Options": {
                                            "colors": [
                                                "#ba141a",
                                                "#0072c6",
                                                "#00bcf2"
                                            ],
                                            "valueColorMapping": []
